{
  "info": {
    "name": "Smart Medical Analyzer API",
    "_postman_id": "smart-medical-api-v2",
    "description": "Complete API collection for Smart Medical Lab Report Analyzer with Python agent integration",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "variable": [
    {
      "key": "baseUrl",
      "value": "http://localhost:5000/api",
      "type": "string"
    },
    {
      "key": "token",
      "value": "",
      "type": "string"
    }
  ],
  "item": [
    {
      "name": "Authentication",
      "item": [
        {
          "name": "Signup",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 201) {",
                  "    const jsonData = pm.response.json();",
                  "    pm.collectionVariables.set('token', jsonData.token);",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"test@example.com\",\n  \"username\": \"testuser\",\n  \"password\": \"password123\"\n}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            },
            "url": {
              "raw": "{{baseUrl}}/auth/signup",
              "host": ["{{baseUrl}}"],
              "path": ["auth", "signup"]
            }
          },
          "response": []
        },
        {
          "name": "Login",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 200) {",
                  "    const jsonData = pm.response.json();",
                  "    pm.collectionVariables.set('token', jsonData.token);",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"test@example.com\",\n  \"password\": \"password123\"\n}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            },
            "url": {
              "raw": "{{baseUrl}}/auth/login",
              "host": ["{{baseUrl}}"],
              "path": ["auth", "login"]
            }
          },
          "response": []
        },
        {
          "name": "Get Me",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}",
                "type": "text"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/auth/me",
              "host": ["{{baseUrl}}"],
              "path": ["auth", "me"]
            }
          },
          "response": []
        }
      ]
    },
    {
      "name": "Report Extraction",
      "item": [
        {
          "name": "Upload PDF Report",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}",
                "type": "text"
              }
            ],
            "body": {
              "mode": "formdata",
              "formdata": [
                {
                  "key": "pdfFile",
                  "type": "file",
                  "src": "/path/to/your/medical_report.pdf",
                  "description": "Select a PDF medical report"
                }
              ]
            },
            "url": {
              "raw": "{{baseUrl}}/extraction/upload",
              "host": ["{{baseUrl}}"],
              "path": ["extraction", "upload"]
            },
            "description": "Upload a PDF medical report. The system will:\n1. Extract medical data using ExtractionAgent\n2. Generate clinical summary using SummaryAgent\n3. Generate recommendations using RecommendationAgent\n4. Identify patient and organize into vault using VaultAgent\n5. Save everything to MongoDB"
          },
          "response": []
        },
        {
          "name": "Get All Reports",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}",
                "type": "text"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/extraction/reports",
              "host": ["{{baseUrl}}"],
              "path": ["extraction", "reports"]
            }
          },
          "response": []
        },
        {
          "name": "Get Report by ID",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}",
                "type": "text"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/extraction/reports/:reportId",
              "host": ["{{baseUrl}}"],
              "path": ["extraction", "reports", ":reportId"],
              "variable": [
                {
                  "key": "reportId",
                  "value": "REPLACE_WITH_REPORT_ID"
                }
              ]
            }
          },
          "response": []
        },
        {
          "name": "Delete Report",
          "request": {
            "method": "DELETE",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}",
                "type": "text"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/extraction/reports/:reportId",
              "host": ["{{baseUrl}}"],
              "path": ["extraction", "reports", ":reportId"],
              "variable": [
                {
                  "key": "reportId",
                  "value": "REPLACE_WITH_REPORT_ID"
                }
              ]
            }
          },
          "response": []
        }
      ]
    }
  ]
}
